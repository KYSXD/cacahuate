<process-spec>
  <process-info>
    <author>categulario</author>
    <date>2018-02-27</date>
    <name>A process that asks a question</name>
    <public>false</public>
    <description>The simplest decision</description>
    <start-node>vhK8ZcR4Z260T3pz</start-node>
  </process-info>
  <process>
    <node id="vhK8ZcR4Z260T3pz" class="simple" />

    <connector from="vhK8ZcR4Z260T3pz" to="57TJ0V3nur6m7wvv" />

    <node id="57TJ0V3nur6m7wvv" class="decision">
      <form-array>
        <form id="fork">
          <input type="radio" name="proceed" required="required">
            <option value="yes" />
            <option value="no" />
          </input>
        </form>
      </form-array>
    </node>

    <connector from="57TJ0V3nur6m7wvv" to="Cuptax0WTCL1ueCy">
      <condition>form#fork[proceed] == "yes"</condition>
    </connector>

    <node id="Cuptax0WTCL1ueCy" class="simple" />
    <connector from="Cuptax0WTCL1ueCy" to="tMdkDIZkDW7GkmDT" />
    <node id="tMdkDIZkDW7GkmDT" class="end" />

    <connector from="57TJ0V3nur6m7wvv" to="mj88CNZUaBdvLV83">
      <condition>form#fork[proceed] == "no"</condition>
    </connector>

    <node id="mj88CNZUaBdvLV83" class="simple" />
    <connector from="mj88CNZUaBdvLV83" to="BCUHAjo4OxtA31NR" />
    <node id="BCUHAjo4OxtA31NR" class="end" />
  </process>
</process-spec>
