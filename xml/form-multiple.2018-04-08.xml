<process-spec>
  <process-info>
    <author>categulario</author>
    <date>2018-04-08</date>
    <name>Con un formulario m√∫ltiple</name>
    <description>Este proceso tiene un formulario que puede enviar muchas copias</description>
    <public>true</public>
  </process-info>
  <process>
    <action id="form-node" >
      <form-array>
        <form id="single-form">
          <input type="text" name="name" required="required" />
        </form>
        <form id="multiple-form" multiple="1-10">
          <input type="text" name="phone" required="required" />
        </form>
      </form-array>
    </action>
  </process>
</process-spec>
